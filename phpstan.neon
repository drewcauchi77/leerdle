includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon
    - vendor/pestphp/pest/extension.neon

parameters:

    paths:
        - app
        - bootstrap/app.php
        - config
        - database
        - public
        - routes
        - tests

    level: max

    tmpDir: /tmp/phpstan

    excludePaths:
        - resources
        - nativephp (?)
